{
    "name": "mnarushevich/laravel-audit-logs",
    "description": "A Laravel package for audit logging",
    "type": "library",
    "autoload": {
        "psr-4": {
            "MNarushevich\\AuditLogs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MNarushevich\\AuditLogs\\AuditLogsServiceProvider"
            ],
            "aliases": {
                "AuditLogs": "MNarushevich\\AuditLogs\\Facades\\AuditLogs"
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan migrate --path=database/migrations"
        ]
    },
    "require": {
        "php": ">=8.4",
        "illuminate/support": "^10.0 || ^12.1",
        "aws/aws-sdk-php": "^3.342",
        "illuminate/database": "^12.1"
    }
}
